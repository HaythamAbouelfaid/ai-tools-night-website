{
    "outputDirectory": ".",
    "buildCommand": "",
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/js/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        },
        {
            "source": "/css/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        }
    ],
    "ignoreCommand": "exit 0"
}